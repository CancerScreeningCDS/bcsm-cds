<http://cancerscreeningcds.github.io/bcsm-cds/PlanDefinition/flow-DecisionToScreen> a fhir:PlanDefinition ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "flow-DecisionToScreen"] ; # 
  fhir:meta [
     fhir:profile ( [
       fhir:v "http://hl7.org/fhir/uv/cpg/StructureDefinition/cpg-computableplandefinition"^^xsd:anyURI ;
       fhir:link <http://hl7.org/fhir/uv/cpg/StructureDefinition/cpg-computableplandefinition>
     ] )
  ] ; # 
  fhir:text [
     fhir:status [ fhir:v "extensions" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n<div>\n    <table class=\"grid dict\">\n        \n        <tr>\n            <th scope=\"row\"><b>Id: </b></th>\n            <td style=\"padding-left: 4px;\">flow-DecisionToScreen</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Url: </b></th>\n            <td style=\"padding-left: 4px;\"><a href=\"PlanDefinition-flow-DecisionToScreen.html\">Decision to Screen</a></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Version: </b></th>\n            <td style=\"padding-left: 4px;\">1.0.0</td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Title: </b></th>\n            <td style=\"padding-left: 4px;\">Decision to Screen</td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Status: </b></th>\n            <td style=\"padding-left: 4px;\">draft</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Experimental: </b></th>\n            <td style=\"padding-left: 4px;\">true</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Type: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                    \n                        \n                        <p style=\"margin-bottom: 5px;\">\n                            <b>system: </b> <span><a href=\"http://terminology.hl7.org/6.2.0/CodeSystem-plan-definition-type.html\">http://terminology.hl7.org/CodeSystem/plan-definition-type</a></span>\n                        </p>\n                        \n                        \n                        <p style=\"margin-bottom: 5px;\">\n                            <b>code: </b> <span>eca-rule</span>\n                        </p>\n                        \n                        \n                    \n                \n                \n            </td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Date: </b></th>\n            <td style=\"padding-left: 4px;\">2024-10-22</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Publisher: </b></th>\n            <td style=\"padding-left: 4px;\">MITRE</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Description: </b></th>\n            <td style=\"padding-left: 4px;\"><div><p>Evaluate criteria to determine age to start and age to stop, screening modality, and screening interval.</p>\n</div></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Knowledge Capability: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                executable \n            </td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Knowledge Representation Level: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                structured\n            </td>\n        </tr>\n        \n\n        \n\n        \n\n        \n\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Copyright: </b></th>\n            <td style=\"padding-left: 4px;\"><div><p>(C) 2024 The MITRE Corporation. All Rights Reserved. Approved for Public Release: 24-2711. Distribution Unlimited. Unless otherwise noted, this work is available under an Apache 2.0 license. It was produced by the MITRE Corporation for the Division of Cancer Prevention and Control, Centers for Disease Control and Prevention in accordance with the Statement of Work, contract number 75FCMC18D0047, task order number 75D30123F17931.</p>\n</div></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Approval Date: </b></th>\n            <td style=\"padding-left: 4px;\">2024-10-22</td>\n        </tr>\n        \n\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Libraries: </b></th>\n            <td style=\"padding-left: 4px;\">\n                <table class=\"grid-dict\">\n                    \n                    <tr>\n                        <td><code>Library/DecisionToScreen|1.0.0</code></td>\n                    </tr>\n                    \n                </table>\n            </td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Actions: </b></th>\n            <td style=\"padding-left: 4px;\">\n                <table class=\"grid-dict\">\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Shared decision making to continue screening based on age:</b> Decision to continue screening mammography in average risk women age 75 years or older should be based on shared decision-making.\n\nThe USPSTF concludes that the evidence is insufficient to determine the balance of benefits and harms of screening mammography in women 75 years or older.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSDMContinueScreeningAge)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/CommunicateSDMContinueScreening|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(SDMContinueScreeningAgeCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(SDMContinueScreeningAgeReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Shared decision making to continue screening based on life expectancy:</b> Decision to continue screening mammography in cases of limited life expectancy should be based on shared decision-making.\n\nACS advises that women should continue screening mammography as long as their overall health is good and they have a life expectancy of 10 years or more.\n\nLife expectancy is generally defined as having greater than a 50% probability of surviving 10 years. A validated tool such as www.eprognosis.com can help guide decision making.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSDMContinueScreeningLifeExp)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/CommunicateSDMContinueScreening|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(SDMContinueScreeningLifeExpCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(SDMContinueScreeningLifeExpReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Shared decision making to delay screening based on lactation status:</b> There is no contraindication to screening mammography during lactation, however sensitivity and specificity are decreased. For average risk patients, consider a short delay in routine breast imaging until after lactation, particularly if they are not planning prolonged breastfeeding.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSDMDelayScreeningLactating)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/CommunicateSDMDelayScreening|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(SDMDelayScreeningLactatingCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(SDMDelayScreeningLactatingReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram starting age 30:</b> An annual screening mammogram starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on known genetic markers or syndromes should get a mammogram every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSGeneticRiskMammo)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSGeneticRiskMammoCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSGeneticRiskMammoTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSGeneticRiskMammoFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSGeneticRiskMammoCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSGeneticRiskMammoReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSGeneticRiskMammoTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening MRI starting age 30:</b> An annual screening MRI starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on known genetic markers or syndromes should get a breast MRI every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSGeneticRiskMri)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSGeneticRiskMriCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSGeneticRiskMriTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSGeneticRiskMriFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSGeneticRiskMriCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSGeneticRiskMriReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSGeneticRiskMriTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to specialist recommendations:</b> A number of other genetic variants are known to be associated with an increased risk of developing of breast cancer. For known personal history of these variants, refer to the consulting geneticist or to high risk breast specialist for guidance on routine screening. \n\nVariants may include ATM, BARD1, CDH1, CHEK2, NF1, PALB2, RAD51C, RAD51D, STK11.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialtyReferralGeneticRisk)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(SpecialtyReferralGeneticRiskCode)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(SpecialtyReferralGeneticRiskCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(SpecialtyReferralGeneticRiskReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram:</b> An annual screening mammogram starting 8 years after radiotherapy and no earlier than age 25 is recommended.\n\nRefer to patient's Oncologic treatment team or Survivorship Care Plan for patient specific guidance.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAfterRT)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAfterRTCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAfterRTTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAfterRTFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAfterRTCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAfterRTReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAfterRTTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening MRI:</b> An annual screening MRI starting 8 years after radiotherapy and no earlier than age 25 is recommended.\n\nRefer to patient's Oncologic treatment team or Survivorship Care Plan for patient specific guidance.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMriAfterRT)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMriAfterRTCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMriAfterRTTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMriAfterRTFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMriAfterRTCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMriAfterRTReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMriAfterRTTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram:</b> Annual screening mammogram after 5 years from breast cancer onset is recomended.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAfterBrca)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAfterBrcaCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAfterBrcaTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAfterBrcaFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAfterBrcaCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAfterBrcaReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAfterBrcaTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to Oncologic care plan:</b> Previous breast cancer within 5 years may require enhanced surveillance including diagnostic mammogram. \n\nRefer to patient's Oncologic treatment team or Survivorship Care Plan for patient specific guidance.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialtyReferralBreastCaWithin5y)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(SpecialtyReferralBreastCaWithin5yCode)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(SpecialtyReferralBreastCaWithin5yCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(SpecialtyReferralBreastCaWithin5yReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram starting age 30:</b> An annual screening mammogram starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on past atypical biopsy should get a mammogram every year, typically starting at age 30.\n\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAtypicalBiopsy)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAtypicalBiopsyCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAtypicalBiopsyTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAtypicalBiopsyFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAtypicalBiopsyCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAtypicalBiopsyReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAtypicalBiopsyTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Perform lifetime risk assessment:</b> Consider performing assessment of lifetime risk. \n\nIf estimated lifetime risk is &gt;20%, patient may be eligible for supplemental screening with annual screening MRI.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSAtypicalBiopsy)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSAtypicalBiopsyCode)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSAtypicalBiopsyCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSAtypicalBiopsyReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening MRI starting age 30:</b> An annual screening MRI starting at age 30 is recommended in patients with past atypical biopsy and lifetime risk &gt; 20-25%.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMriAtypicalBiopsyHighRisk)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsHighRiskExclusionsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMriAtypicalBiopsyHighRiskCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMriAtypicalBiopsyHighRiskTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMriAtypicalBiopsyHighRiskFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMriAtypicalBiopsyHighRiskCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMriAtypicalBiopsyHighRiskReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMriAtypicalBiopsyHighRiskTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram starting age 30:</b> An annual screening mammogram starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on certain risk factors should get a mammogram every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoHighRisk)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoHighRiskCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoHighRiskTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoHighRiskFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoHighRiskCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoHighRiskReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoHighRiskTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening MRI starting age 30:</b> An annual screening MRI starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on certain risk factors should get a breast MRI every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMriHighRisk)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMriHighRiskCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMriHighRiskTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMriHighRiskFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMriHighRiskCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMriHighRiskReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMriHighRiskTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammo starting age 30:</b> An annual screening mammogram starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on certain risk factors should get a mammogram every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSFirstDegGeneticMammo)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSFirstDegGeneticMammoCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSFirstDegGeneticMammoTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSFirstDegGeneticMammoFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSFirstDegGeneticMammoCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSFirstDegGeneticMammoReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSFirstDegGeneticMammoTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening MRI starting age 30:</b> An annual screening MRI starting at age 30 is recommended.\n\nWomen who are at higher than average risk for breast cancer based on certain risk factors should get a breast MRI every year, typically starting at age 30.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSFirstDegGeneticMri)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSFirstDegGeneticMriCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSFirstDegGeneticMriTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSFirstDegGeneticMriFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSFirstDegGeneticMriCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSFirstDegGeneticMriReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSFirstDegGeneticMriTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Perform lifetime risk assessment:</b> Consider performing assessment of lifetime risk, as defined by models largely dependent on family history.\n\nIf estimated lifetime risk is &gt;20%, based on informal assessment or assessment using validated risk model, then patient is eligible for annual screening mammogram starting at age 30 and supplemental screening with annual screening MRI. Otherwise, patient can be offered annual screening mammogram starting at age 40.\n\nValidated tools include BRCAPRO, Claus, BOADICEA [Breast and Ovarian Analysis of Disease Incidence and Carrier Estimation Algorithm], and Tyrer-Cuzick.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsBreastImagingHeterogenouslyOrExtremelyDense)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            reasonCode[0].coding[0]: <i>(BreastImagingHeterogenouslyOrExtremelyDenseReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Perform lifetime risk assessment:</b> Consider performing assessment of lifetime risk, as defined by models largely dependent on family history.\n\nIf estimated lifetime risk is &gt;20%, based on informal assessment or assessment using validated risk model, then patient is eligible for annual screening mammogram starting at age 30 and supplemental screening with annual screening MRI. Otherwise, patient can be offered annual screening mammogram starting at age 40.\n\nValidated tools include BRCAPRO, Claus, BOADICEA [Breast and Ovarian Analysis of Disease Incidence and Carrier Estimation Algorithm], and Tyrer-Cuzick.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsCompellingFamilyHistory)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(CompellingFamilyHistoryCode)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(CompellingFamilyHistoryCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(CompellingFamilyHistoryReason)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Biennial screening mammo starting age 40:</b> Biennial screening mammogram starting at age 40\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsUSPSTFRecommendation)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsSpecialPopulationsRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(USPSTFRecommendationCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(USPSTFRecommendationTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(USPSTFRecommendationFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(USPSTFRecommendationCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(USPSTFRecommendationReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(USPSTFRecommendationTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram starting age 40:</b> Women between 40 and 44 have the option to start screening with a mammogram every year.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAvgRiskAgeUnder45)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsAverageRiskRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAvgRiskAgeUnder45Code)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAvgRiskAgeUnder45Timing)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAvgRiskAgeUnder45Flag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAvgRiskAgeUnder45Code)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAvgRiskAgeUnder45Reason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAvgRiskAgeUnder45TimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Annual screening mammogram starting age 40:</b> Annual screening mammogram is recommended for women 45 to 54.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAvgRiskAge45to54)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsAverageRiskRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAvgRiskAge45to54Code)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAvgRiskAge45to54Timing)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAvgRiskAge45to54Flag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAvgRiskAge45to54Code)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAvgRiskAge45to54Reason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAvgRiskAge45to54TimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Biennial screening mammogram after age 54:</b> Biennial screening mammogram is recommended after age 54.\n\nWomen 55 and older can switch to a mammogram every other year, or they can choose to continue yearly mammograms. Screening should continue as long as a woman is in good health and is expected to live at least 10 more years.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsACSMammoAvgRiskAgeOver54)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsAverageRiskRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(ACSMammoAvgRiskAgeOver54Code)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(ACSMammoAvgRiskAgeOver54Timing)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(ACSMammoAvgRiskAgeOver54Flag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(ACSMammoAvgRiskAgeOver54Code)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(ACSMammoAvgRiskAgeOver54Reason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(ACSMammoAvgRiskAgeOver54TimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Biennial screening mammogram starting age 40:</b> Biennial screening mammogram starting at age 40\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsUSPSTFAvgRisk)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsAverageRiskRecommendations)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(USPSTFAvgRiskCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(USPSTFAvgRiskTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(USPSTFAvgRiskFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(USPSTFAvgRiskCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(USPSTFAvgRiskReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(USPSTFAvgRiskTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                </table>\n            </td>\n        </tr>\n        \n    </table>\n</div>\n</div>"^^rdf:XMLLiteral
