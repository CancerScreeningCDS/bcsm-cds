     fhir:status [ fhir:v "extensions" ] ;
     fhir:div "<div xmlns=\"http://www.w3.org/1999/xhtml\">\n<div>\n    <table class=\"grid dict\">\n        \n        <tr>\n            <th scope=\"row\"><b>Id: </b></th>\n            <td style=\"padding-left: 4px;\">flow-FollowupScreeningResult</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Url: </b></th>\n            <td style=\"padding-left: 4px;\"><a href=\"PlanDefinition-flow-FollowupScreeningResult.html\">Followup Screening Result</a></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Version: </b></th>\n            <td style=\"padding-left: 4px;\">1.0.0</td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Title: </b></th>\n            <td style=\"padding-left: 4px;\">Followup Screening Result</td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Status: </b></th>\n            <td style=\"padding-left: 4px;\">draft</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Experimental: </b></th>\n            <td style=\"padding-left: 4px;\">true</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Type: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                    \n                        \n                        <p style=\"margin-bottom: 5px;\">\n                            <b>system: </b> <span><a href=\"http://terminology.hl7.org/6.2.0/CodeSystem-plan-definition-type.html\">http://terminology.hl7.org/CodeSystem/plan-definition-type</a></span>\n                        </p>\n                        \n                        \n                        <p style=\"margin-bottom: 5px;\">\n                            <b>code: </b> <span>eca-rule</span>\n                        </p>\n                        \n                        \n                    \n                \n                \n            </td>\n        </tr>\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Date: </b></th>\n            <td style=\"padding-left: 4px;\">2024-10-22</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Publisher: </b></th>\n            <td style=\"padding-left: 4px;\">MITRE</td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Description: </b></th>\n            <td style=\"padding-left: 4px;\"><div><p>Determines next steps, if any, based on result of screening imaging or biopsy. This may be triggered by a diagnostic report being received by the system or an imaging BI-RADS result being documented in a structured manner.</p>\n</div></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Knowledge Capability: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                executable \n            </td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Knowledge Representation Level: </b></th>\n            <td style=\"padding-left: 4px;\">\n                \n                structured\n            </td>\n        </tr>\n        \n\n        \n\n        \n\n        \n\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Copyright: </b></th>\n            <td style=\"padding-left: 4px;\"><div><p>(C) 2024 The MITRE Corporation. All Rights Reserved. Approved for Public Release: 24-2711. Distribution Unlimited. Unless otherwise noted, this work is available under an Apache 2.0 license. It was produced by the MITRE Corporation for the Division of Cancer Prevention and Control, Centers for Disease Control and Prevention in accordance with the Statement of Work, contract number 75FCMC18D0047, task order number 75D30123F17931.</p>\n</div></td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Approval Date: </b></th>\n            <td style=\"padding-left: 4px;\">2024-10-22</td>\n        </tr>\n        \n\n        \n\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Libraries: </b></th>\n            <td style=\"padding-left: 4px;\">\n                <table class=\"grid-dict\">\n                    \n                    <tr>\n                        <td><code>Library/FollowupScreeningResult|1.0.0</code></td>\n                    </tr>\n                    \n                </table>\n            </td>\n        </tr>\n        \n\n        \n        <tr>\n            <th scope=\"row\"><b>Actions: </b></th>\n            <td style=\"padding-left: 4px;\">\n                <table class=\"grid-dict\">\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to Radiologist recommendations:</b> Refer to Radiologist recommendations for further evaluation in BI-RADS 0 screening results.\n\nAdditional work-up including comparison to prior mammograms or diagnostic imaging may be indicated.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupBirads0Result)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupBirads0ResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupBirads0ResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupBirads0ResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupBirads0ResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupBirads0ResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupBirads0ResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to Radiologist recommendations:</b> Probably benign finding. Refer to Radiologist recommendations for interval imaging in BI-RADS 3 screening results.\n\nPeriodic monitoring, including diagnostic mammogram or ultrasound, is warranted to assess for changes. Monitoring may be recommended at 6, 12 and 24 months until the finding is felt to be stable or resolving or is transitioned to a higher BI-RADS categorization.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupBirads3Result)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupBirads3ResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupBirads3ResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupBirads3ResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupBirads3ResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupBirads3ResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupBirads3ResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer for breast biopsy:</b> Refer for breast biopsy.\n\nInitial modality will typically be image-guided core needle biopsy (guided by mammography, ultrasound, or MRI). In certain conditions, an initial excisional biopsy may be preferable. These may include lesions anatomically unsuitable for needle biopsy or patient anatomic considerations. Refer to resulting Radiologist for recommendations.\n\nIn imaging findings that have previously been biopsied and found to be benign, consider referral for excisional biopsy if there have been suspicious interval changes in the lesion.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupBirads4Or5Result)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupBirads4Or5ResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupBirads4Or5ResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupBirads4Or5ResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupBirads4Or5ResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupBirads4Or5ResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupBirads4Or5ResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Continue routine screening:</b> Benign lesions without atypia or discordance with imaging can return to routine screening.\n\nCertain specific benign histologies may require additional tissue, including mucin-producing lesions, potential phyllodes tumor, papillary lesions, or radial scars. Refer to pathologist for further recommendations.\n\nOther specific histologies may be considered for excisional biopsy and can be referred to a breast surgeon for evaluation. These may include flat epithelial atypia [FEA], papillary and/or sclerosing lesion, and fibroepithelial lesions (fibroadenoma vs benign phyllodes tumor).\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupBenignBiopsyResult)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupBenignBiopsyResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupBenignBiopsyResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupBenignBiopsyResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupBenignBiopsyResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupBenignBiopsyResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupBenignBiopsyResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to breast surgeon:</b> Atypical fndings, including ADH and non-classic LCIS, should be referred for potential excision.\n\nAtypical ductal hyperplasia (ADH) and non-classic lobular carcinoma in situ (LCIS), including pleomorphic or florid LCIS, should be referred to breast surgeon for potential excision.\n\nClassic LCIS and atypical lobular hyperplasia (ALH) are typically candidates for observation with clinical and imaging follow-up but may benefit from excision in certain circumstances.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupAtypicalBiopsyResult)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupAtypicalBiopsyResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupAtypicalBiopsyResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupAtypicalBiopsyResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupAtypicalBiopsyResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupAtypicalBiopsyResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupAtypicalBiopsyResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to breast cancer specialist:</b> Referral to breast surgery or medical oncology as appropriate.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupMalignantBiopsyResult)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupMalignantBiopsyResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupMalignantBiopsyResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupMalignantBiopsyResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupMalignantBiopsyResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupMalignantBiopsyResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupMalignantBiopsyResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to breast surgeon:</b> Indeterminate fndings should be referred for potential excisional biopsy.\n\nA core needle biopsy specimen may be non-diagnostic for a number of reasons (insufficient material, lack of calcifications, hemorrhage).\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupIndeterminateBiopsyResult)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupIndeterminateBiopsyResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupIndeterminateBiopsyResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupIndeterminateBiopsyResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupIndeterminateBiopsyResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupIndeterminateBiopsyResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupIndeterminateBiopsyResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                    <tr>\n                        <td>\n                            \n                            <b> Refer to breast surgeon:</b> Benign histology findings on core needle biopsy that are discordant with imaging findings should be referred for potential excisional biopsy.\n\nA histologic result may be discordant if the pathology result does not account for imaging findings that are suspicious for malignancy. The designation of discordance may require input from radiologist, breast surgeon, and breast pathologist. \n\nA repeat needle biopsy may be considered if the initial biopsy procedure was felt to be inadequate. Refer to radiologist recommendations for guidance.\n                            \n                            <br/>\n                            \n                            \n                            <b>If:</b>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(NoErrorsHaveOccurred)</i>\n                            <br/>\n                            \n                            <i>Applicability:</i>\n                            \n                            <i>(ExistsFollowupDiscordantBiopsyResult)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                            <b>Then:</b> <i>Definition:</i> <code>http://cancerscreeningcds.github.io/bcsm-cds/ActivityDefinition/BreastCancerScreeningServiceRequest|1.0.0</code><br/>\n                            \n                            <i>Dynamic Values:</i><br/>\n                            \n                            %action.code[0].coding[0]: <i>(FollowupDiscordantBiopsyResultCode)</i>\n                            <br/>\n                            \n                            %action.timingTiming: <i>(FollowupDiscordantBiopsyResultTiming)</i>\n                            <br/>\n                            \n                            %action.extension: <i>(FollowupDiscordantBiopsyResultFlag)</i>\n                            <br/>\n                            \n                            code.coding[0]: <i>(FollowupDiscordantBiopsyResultCode)</i>\n                            <br/>\n                            \n                            reasonCode[0].coding[0]: <i>(FollowupDiscordantBiopsyResultReason)</i>\n                            <br/>\n                            \n                            occurrenceDateTime: <i>(FollowupDiscordantBiopsyResultTimingEvent)</i>\n                            <br/>\n                            \n                            \n                            \n                            \n                        </td>\n                    </tr>\n                    \n                </table>\n            </td>\n        </tr>\n        \n    </table>\n</div>\n</div>"^^rdf:XMLLiteral
  ] ; # 
