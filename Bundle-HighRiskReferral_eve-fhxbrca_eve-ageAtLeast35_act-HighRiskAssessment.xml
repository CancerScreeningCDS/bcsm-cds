              <label value="BCSC Risk Calculator"/>
              <display value="Breast Cancer Surveillance Consortium (BCSC) Risk Calculator"/>
              <url value="https://tools.bcsc-scc.ucdavis.edu/BC5yearRisk"/>
            </documentation>
            <resource>
              <reference value="ServiceRequest/468"/>
            </resource>
          </action>
        </action>
